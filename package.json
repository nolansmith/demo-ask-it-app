{
  "name": "ask-it-app",
  "version": "1.0.0",
  "description": "React SPA for Q/A Business Case",
  "main": "index.js",
  "scripts": {
    "migrate": "sequelize db:migrate && sequelize db:seed:all",
    "rollback": "sequelize db:migrate:undo:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "webpack --config ./src/webapp/config/webpack.config.js",
    "start:prod": "npm run pack && npm run server",
    "start:dev": "concurrently \"npm run graphql\" \"webpack-dev-server --config ./src/webapp/config/webpack.dev.config.js --port 8080\"",
    "profile": "webpack --profile --json --config ./src/webapp/config/webpack.config.js > stats.json",
    "analyzer": "webpack-bundle-analyzer stats.json",
    "graphql": "cross-env NODE_ENV=development nodemon --plugins require-context-hook --exec babel-node --watch ./src/server/services/graphql ./src/server/services/graphql/server.js",
    "server": "cross-env NODE_ENV=production nodemon --plugins require-context-hook --exec babel-node --watch ./src/server ./src/server/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo-boost": "^0.4.4",
    "apollo-link-error": "^1.1.12",
    "apollo-server-express": "^2.9.0",
    "axios": "^0.19.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "colors": "^1.4.0",
    "core-js": "^3.6.3",
    "express": "^4.17.1",
    "graphql": "^14.5.3",
    "graphql-cli": "^3.0.14",
    "graphql-tools": "^4.0.5",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "mysql2": "^2.0.0",
    "node-fetch": "^2.6.0",
    "popper.js": "^1.15.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.8.0",
    "react-loading": "^2.0.3",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "stripe": "^7.15.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/node": "^7.7.4",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "8.0.5",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-preset-react-app": "7.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.2",
    "copy-webpack-plugin": "^5.0.5",
    "cross-env": "^6.0.3",
    "css-loader": "^3.0.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "html-webpack-plugin": "3.2.0",
    "nodemon": "^2.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpackbar": "^4.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": "3.6.3"
        }
      ],
      "@babel/preset-react"
    ]
  }
}
