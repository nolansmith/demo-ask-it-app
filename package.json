{
  "name": "ask-it-app",
  "version": "1.0.0",
  "description": "React SPA for Q/A Business Case",
  "main": "index.js",
  "scripts": {
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "unseed": "sequelize db:seed:undo:all",
    "rollback": "sequelize db:migrate:undo:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config ./src/webapp/config/webpack.config.js ",
    "build:server": "babel ./src/server --out-dir ./prod/server --plugins=@babel/plugin-transform-regenerator --minified",
    "build:server:dev": "babel ./src/server --out-dir ./dev/server --plugins=@babel/plugin-transform-regenerator --minified",
    "start": "npm run server:prod:heroku",
    "start:prod": "npm run build && npm run server:prod",
    "start:dev": "concurrently \"npm run server:dev\" \"webpack-dev-server --config ./src/webapp/config/webpack.dev.config.js --port 8080\"",
    "profile": "webpack --profile --json --config ./src/webapp/config/webpack.config.js > stats.json",
    "analyzer": "webpack-bundle-analyzer stats.json",
    "graphql": "cross-env NODE_ENV=development nodemon --exec babel-node --watch ./src/server/services/graphql ./src/server/services/graphql/server.js",
    "server:prod": "cross-env DEPLOYMENT_HAS_VARS=no NODE_ENV=production node ./prod/server/index.js",
    "server:prod:heroku": "cross-env DEPLOYMENT_HAS_VARS=yes NODE_ENV=production node ./prod/server/index.js",
    "server:dev": "cross-env DEPLOYMENT_HAS_VARS=no NODE_ENV=development nodemon --exec babel-node ./src/server/index.dev.js"
  },
  "keywords": [],
  "author": "Nolan Smith",
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-server-express": "^2.9.16",
    "axios": "^0.19.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "graphql": "^14.5.3",
    "graphql-cli": "^3.0.14",
    "graphql-tools": "^4.0.5",
    "helmet": "^3.21.2",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mysql2": "^2.1.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.2",
    "popper.js": "^1.16.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "react-loading": "^2.0.3",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.3",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.8.3",
    "@babel/plugin-transform-regenerator": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "8.0.5",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-preset-react-app": "7.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.2",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-webpack-plugin": "3.2.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpackbar": "^4.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": "3.6.3"
        }
      ],
      "@babel/preset-react"
    ]
  },
  "heroku-run-build-script": true
}
